"use strict";

var mvc = require('express-mvc');

class <%=name%>Provider extends mvc.Provider
{
    constructor()
    {
        super("<%= name.toLowerCase() %>");

        this.order = 0;

        this.requires([
            'logger'
        ]);

        this.inside(ENV_CLI);
    }

    /**
     * Register any classes or instances with the application.
     * @returns void
     */
    register(app)
    {
        //
    }
}

module.exports = new <%=name%>Provider();